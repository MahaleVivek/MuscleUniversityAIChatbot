{
  "version": "0.2.0",
  "envFile": "${workspaceFolder}/.env",
  "configurations": [
    {
      "name": "Lanarky: Conversation Chain",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "cwd": "${workspaceFolder}/examples",
      "args": [
        "app.conversation_chain:app",
        "--reload",
        "--host",
        "localhost",
        "--port",
        "8000"
      ]
